<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.king.framework.counter.mapper.SysCounterMapper">
    <select id="selectSysCounter"  parameterType="String"  resultType="com.king.framework.counter.entity.SysCounter">
        SELECT a.* FROM t_sys_counter a
        WHERE a.COUNTER_NAME = #{counterName}
    </select>
</mapper>